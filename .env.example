# Telegram bot token from BotFather
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Perplexity API key
PERPLEXITY_API_KEY=your_perplexity_api_key_here

# Webhook URL for production deployment (optional, if not set will use polling mode)
# WEBHOOK_URL=https://your-app-url.com

# Database URL (optional, defaults to the one in docker-compose)
# DATABASE_URL=postgresql://postgres:postgres@db:5432/perplexity
